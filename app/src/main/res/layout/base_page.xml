<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".BasePage"
    tools:layout_editor_absoluteY="25dp">


    <TextView
        android:id="@+id/nameText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="40dp"
        android:text="Way to go ! You're on a hot 3-week work out streak"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#808080"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/nameText" />

    <View
        android:id="@+id/view10"
        android:layout_width="170dp"
        android:layout_height="2dp"
        android:layout_marginEnd="34dp"
        android:background="#808080"
        android:backgroundTint="#808080"
        app:layout_constraintBottom_toBottomOf="@+id/textView12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/textView12" />

    <TextView
        android:id="@+id/nameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="74dp"
        android:layout_marginBottom="1dp"
        android:text="Welcome YOUNG WOLF"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/nameText2"
        app:layout_constraintStart_toStartOf="@+id/nameText2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginTop="157dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/login_cylinder_shape_with_frame"
        android:text="GUIDED TRAINING"
        android:textAlignment="center"
        android:textColor="#808080"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintEnd_toStartOf="@+id/view10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="304dp"
        android:layout_height="0dp"
        android:layout_marginStart="105dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="507dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:srcCompat="@drawable/base_page" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="343dp"
        android:layout_height="438dp"
        android:layout_marginTop="350dp"
        android:layout_marginBottom="65dp"
        android:background="@drawable/main_page_around_menu"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <!-- imageView7 -->
        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/main_page_around_menu_photo"
            android:contentDescription="Description for imageView7"
            android:src="@drawable/base_page_first_row" />

        <!-- textView15 -->
        <TextView
            android:id="@+id/textView15"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/main_page_cylinder_with_frame"
            android:text="EXERCÄ°SES"
            android:textAlignment="center"
            android:textColor="#FFFFFF" />

        <!-- imageView8 -->
        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="@drawable/main_page_around_menu_photo"
            android:contentDescription="Description for imageView8"
            android:onClick="goSupplements"
            android:src="@drawable/base_page_second_row" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/main_page_cylinder_with_frame"
            android:onClick="goSupplements"
            android:text="SUPPLEMENT"
            android:textAlignment="center"
            android:textColor="#FFFFFF" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="@drawable/main_page_around_menu_photo"
            android:contentDescription="Description for imageView10"
            android:onClick="goProfile"
            android:src="@drawable/base_page_third_row" />

        <!-- textView18 -->
        <TextView
            android:id="@+id/textView18"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/main_page_cylinder_with_frame"
            android:onClick="goProfile"
            android:text="PROFILE"
            android:textAlignment="center"
            android:textColor="#FFFFFF" />

    </LinearLayout>

    <View
        android:id="@+id/view"
        android:layout_width="2dp"
        android:layout_height="150dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="188dp"
        android:layout_marginEnd="73dp"
        android:layout_marginBottom="528dp"
        android:background="#808080"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>